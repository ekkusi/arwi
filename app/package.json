    "@types/react": "~18.2.14",
    "cross-env": "^7.0.3",
    "eslint-plugin-react-native": "^4.0.0"
  },
  "resolutions": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  },
  "private": true
