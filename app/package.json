{
  "name": "arwi-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start-no-backend": "cross-env BACKEND_API_URL=https://arwi-test.lm.r.appspot.com npx expo start",
    "start-mac": "BACKEND_API_URL=http://$(ipconfig getifaddr en0):4000 npx expo start",
    "start-win": "for /f \"tokens=2 delims=[]\" %a in ('ping -n 1 -4 \"%computername%\"') do set BACKEND_API_URL=http://%a:4000 && npx expo start",
    "android": "expo start --android",
    "link": "react-native link",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint:fix": "eslint --fix .",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/elements": "^1.3.18",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@types/react-native-vector-icons": "^6.4.13",
    "arwi-backend": "*",
    "date-fns": "^2.30.0",
    "expo": "~48.0.15",
    "expo-constants": "~14.2.1",
    "expo-secure-store": "~12.1.1",
    "expo-status-bar": "~1.4.4",
    "graphql": "^16.6.0",
    "i18next": "^22.5.0",
    "i18next-gridly-backend": "^1.0.6",
    "react": "18.2.0",
    "react-i18next": "^12.3.1",
    "react-native": "0.71.8",
    "react-native-date-picker": "^4.2.13",
    "react-native-dotenv": "^3.4.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-pager-view": "6.1.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-vector-icons": "^9.2.0",
    "victory-native": "^36.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@types/react": "~18.0.14",
    "cross-env": "^7.0.3",
    "eslint-plugin-react-native": "^4.0.0"
  },
  "resolutions": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  },
  "private": true
}
