{
  "extends": [
    "../.eslintrc.json"
  ],
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "react/no-unstable-nested-components": [
      "error",
      {
        "allowAsProps": true
      }
    ],
    "react-native/no-unused-styles": "error",
    "react-native/no-raw-text": ["error", {"skip": ["CText"]}],
    "react-hooks/rules-of-hooks" : "error",
    "global-require": "off",
  },
  "ignorePatterns": [
    "metro.config.js",
    "babel.config.js",
    "jest.config.js"
  ]
}