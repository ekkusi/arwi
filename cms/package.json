{
  "name": "arwi-cms",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "bash scripts/kill-process.sh && sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy",
    "export-and-convert": "npx sanity dataset export production ./export/data.tar.gz && cd export && tar -xzvf data.tar.gz && mv ./*/* ./ && ndjson-reduce < data.ndjson > data.json && find . ! -name 'data.json' -type f -exec rm -f {} + && yarn transform-export",
    "transform-export": "ts-node ./utils/transformExport.ts"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/color-input": "^3.1.0",
    "@sanity/vision": "^3.16.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "sanity": "^3.16.7",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "@types/styled-components": "^5.1.26",
    "ndjson-cli": "^0.3.1",
    "ts-node": "^10.9.1"
  }
}
